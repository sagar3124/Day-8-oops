#3. Write a “person” class to hold all the details.
  class Person{
    constructor(name,age,qualification,heigth,weigth,city,state){
        this.name=name;
        this.age=age;
        this.qualification=qualification;
        this.heigth=heigth;
        this.weigth=weigth;
        this.city=city;
        this.state=state;
    }
    details(){
        console.log(`
        name:${this.name}
        age:${this.age}
        qualification:${this.qualification}
        height:${this.heigth}
        weight:${this.weigth}
        city:${this.city}
        state:${this.state}`)
    }
  }
  let Person1=new Person("Sagar.rao","23","bcom","6","85","Travandrum","Kerala")
  Person1.details()
  
#OUTPUT
        #name:Sagar.rao
        #age:23
        #qualification:bcom
        #height:6
        #weight:85
        #city:Travandrum
        #state:Kerala
