#1. https://github.com/rvsp/typescript-oops/blob/master/Practice/Movie.md

class movie{
  constructor(title,studio,rating){
      this.title=title;
      this.studio=studio;
      this.rating=rating;
      if(this.rating===undefined){this.rating="PG"}
  }
  display (){
      console.log(`${this.title},${this.studio},${this.rating}`)
  }
 movieobject(){
  return ({title:this.title,
      studio:this.studio,
      rating:this.rating})

 }
 getgetPG(arr)
 {
  let data=arr.filter(film=>film.rating=="PG")
   console.log(data.map(b=>b.title))
 }
}
 let Movie1=new movie("Love Mocktail 2","KRG Studios")
 Movie1.display()
let Movie2=new movie("KGF","hombale films","PG")
let Movie3=new movie("Badava Rascal","anaDM studiopurna","PG")
let Movie4=new movie("Madagaja","xxxxKanteerva Studio","PG")
let film=new movie("Yuvarathnaae","Hombale Films","PG13")
film.display()
let arr=[]
 arr.push(Movie1.movieobject())
 arr.push(Movie2.movieobject())
 arr.push(Movie3.movieobject())
 arr.push(Movie4.movieobject())
 console.log(arr)

#OUT-PUT
# [
# { title: 'Love Mocktail 2', studio: 'KRG Studios', rating: 'PG' },    
#  { title: 'KGF', studio: 'hombale films', rating: 'PG' },
# { title: 'Badava Rascal', studio: 'anaDM studiopurna', rating: 'PG' },
#  { title: 'Madagaja', studio: 'xxxxKanteerva Studio', rating: 'PG' }   
